{
	"manifest_version": 3,
	"name": "unload-tab-chrome-extension",
	"version": "1.0",
	"description": "Unload (discard) the active tab with a keyboard shortcut",
	"permissions": ["tabs"],
	"background": {
		"service_worker": "background.js"
	},
	"commands": {
		"discard_active_tab": {
			"suggested_key": {
				"default": "Ctrl+Shift+U"
			},
			"description": "Discard the active tab"
		}
	}
}
